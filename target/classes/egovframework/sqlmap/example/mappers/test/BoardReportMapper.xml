<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="egovframework.sth.domain.board.mapper.BoardReportMapper">
	<resultMap type="egovframework.sth.domain.board.domain.BoardReportDTO" id="BoardReportDTO"/>
	<resultMap type="egovframework.sth.domain.board.domain.BoardDTO" id="BoardDTO"/>

	<insert id="insReport">
		INSERT INTO report
		(b_no, m_no, rp_ctnt)
		VALUES
		(#{b_no}, #{m_no}, #{rp_ctnt})
	</insert>
	
	<select id="selReportList" resultMap="BoardDTO">
	
	</select>
	<delete id="delReport">
		DELETE report
		WHERE rp_no = #{rp_no}
	</delete>
</mapper>
